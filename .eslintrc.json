{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": [
      0
    ],
    "react/jsx-one-expression-per-line": [
      0
    ],
    "react/no-unescaped-entities": [
      0
    ],
    "operator-linebreak": [
      0
    ],
    "implicit-arrow-linebreak": [
      0
    ],
    "import/no-named-as-default-member": [
      0
    ],
    "no-shadow": [
      0
    ],
    "react/prop-types": [
      0
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
